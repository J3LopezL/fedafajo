{
  "name": "jest-puppeteer-example",
  "version": "1.0.0",
  "description": "Automated browser testing example using Jest and Puppeteer",
  "main": "index.js",
  "scripts": {
    "test": "jest --runInBand",
    "test:voyeur": "HEADLESS=false jest --runInBand",
    "test:user": "jest user.spec.js",
    "test:page": "jest page.spec.js",
    "test:tag": "jest tag.spec.js",
    "test:post": "jest post.spec.js",
    "test:login": "jest login.spec.js"
  },
  "jest": {
    "preset": "jest-puppeteer"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "faker": "^5.5.3",
    "jest": "^26.6.3",
    "jest-puppeteer": "^5.0.3",
    "puppeteer": "^9.1.1"
  },
  "keywords": [
    "testing",
    "puppeteer",
    "jest",
    "chromium"
  ],
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.1",
    "chai": "^4.3.4",
    "expect-puppeteer": "^5.0.3",
    "puppeteer-extra-plugin-adblocker": "^2.11.11",
    "puppeteer-extra-plugin-stealth": "^2.7.8"
  }
